var SAS=(function(a,b){a.subscribe("/trigger/inline",function(){var f=document.getElementsByTagName("*");
var d;
var e=0;
for(d in f){if(f[d].hasAttribute&&f[d].hasAttribute("data-include")){if(f[d].hasAttribute("data-matchMedia")){if(!window.matchMedia(f[d].getAttribute("data-matchMedia")).matches){continue
}}c(f[d],f[d].getAttribute("data-include"))
}}function c(j,h){if(!h||!h.length){return
}e+=1;
var i=new XMLHttpRequest(),g=0;
i.onreadystatechange=function(){if(i.readyState==4){g=i.status;
e-=1
}if(i.readyState==4&&g==200){b(j).replaceWith(i.responseText)
}if(e===0){a.publish("/trigger/inline")
}};
try{i.open("GET",h+"?"+Date.now(),true);
i.send()
}catch(k){}}});
return a
}(SAS,jQuery));
jQuery(document).ready(function(a){SAS.publish("/trigger/inline")
});