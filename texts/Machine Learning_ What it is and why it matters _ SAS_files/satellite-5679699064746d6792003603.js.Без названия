_satellite.pushAsyncScript(function(event, target, $variables){
  var languageCode = _satellite.getVar('sascom-page-language');

var refNode = document.querySelector('div#subfooter div.text p');
var firstScript   = document.createElement("span");
firstScript.id = "teconsent";
if (refNode) {
refNode.insertBefore(firstScript, refNode.childNodes[4]);
}
var secondScript   = document.createElement("script");
secondScript.type  = "text/javascript";
secondScript.setAttribute('async', 'async');
secondScript.setAttribute('crossorigin', '');
secondScript.src = '//consent.truste.com/notice?domain=sas.com&c=teconsent&text=true';
if (languageCode && languageCode.length == 2) {
	secondScript.src += '&language=' + languageCode;
}
if (refNode) {
refNode.insertBefore(secondScript, refNode.childNodes[5]);
}
});
