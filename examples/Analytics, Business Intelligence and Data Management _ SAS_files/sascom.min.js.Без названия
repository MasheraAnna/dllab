jQuery(document).ready(function(){var f=jQuery("a.async-list");
var g=[];
jQuery.each(f,function(k,j){var l=jQuery.ajax({url:jQuery(this).attr("href"),type:"GET",dataType:"html"});
l.done(function(i){jQuery(j).replaceWith(i)
});
l.fail(function(i,m){jQuery(j).replaceWith("");
console.log("Request failed: "+m)
});
g.push(l)
});
var e=function(k){var j=jQuery("#li_"+currentContent[0].id);
var m=jQuery("section.#"+currentContent[0].id);
var o=j.parents(".tabctrl");
var n=o.find(".mod-tabs li.active");
n.removeClass("active");
var l=j.find(".tabctrl-content:visible");
l.removeClass("active");
j.addClass("active");
var i=o.find(j.first().attr("href"));
i.addClass("active");
m.addClass("active")
};
var d=function(){var j=decodeURIComponent(window.location.hash)+"scroll";
var i=jQuery(j.toLowerCase());
if(i.parents("section[id$='tab']").length){currentContent=i.parents("section[id$='tab']");
e(currentContent[0].id)
}setTimeout(function(){if(jQuery(j).length>0){var k=jQuery(j).offset().top-50;
jQuery("html, body").animate({scrollTop:k},1000,"swing")
}},300);
jQuery(".listgroup").trigger("listloaded")
};
var h=function(j){var m=j;
if(jQuery(m).parents(".subtabs").length){}else{if(location.pathname.replace(/^\//,"")==m.pathname.replace(/^\//,"")&&location.hostname==m.hostname){var l=jQuery(m).attr("href")+"scroll";
try{if(l.length){var i;
if(jQuery(".header.fixed").length){i=jQuery(l).offset().top
}else{i=jQuery(l).offset().top-150
}jQuery("html, body").animate({scrollTop:i},1000,"swing")
}}catch(k){console.log(k.message)
}}}};
var c=function(){if(typeof jmpDesign!="undefined"&&typeof jmpDesign.scrollToHash=="function"){jmpDesign.scrollToHash()
}else{if(typeof pblsDesign!="undefined"&&typeof pblsDesign.scrollToHash=="function"){pblsDesign.scrollToHash()
}else{if(typeof saswwwDesign!="undefined"&&typeof saswwwDesign.scrollToHash=="function"){saswwwDesign.scrollToHash()
}else{if(typeof sascampaignsDesign!="undefined"&&typeof sascampaignsDesign.scrollToHash=="function"){sascampaignsDesign.scrollToHash()
}else{d()
}}}}};
var a=function(){if(typeof jmpDesign!="undefined"&&typeof jmpDesign.scrollToListGroup=="function"){jmpDesign.scrollToListGroup(this)
}else{if(typeof pblsDesign!="undefined"&&typeof pblsDesign.scrollToListGroup=="function"){pblsDesign.scrollToListGroup(this)
}else{if(typeof saswwwDesign!="undefined"&&typeof saswwwDesign.scrollToListGroup=="function"){saswwwDesign.scrollToListGroup(this)
}else{if(typeof sascampaignsDesign!="undefined"&&typeof sascampaignsDesign.scrollToListGroup=="function"){sascampaignsDesign.scrollToListGroup(this)
}else{h(this)
}}}}};
var b=function(){jQuery(this).toggleClass("show");
jQuery(this).next("span.abstract").toggleClass("show");
return false
};
jQuery(".listgroup").on("click","a[href*=#]:not([href=#])",a);
jQuery("body").on("click","a[href*=#]:not([href=#])",a);
jQuery(".listgroup").on("click","button.abstract-toggle",b);
jQuery(".assetlist").on("click","button.abstract-toggle",b);
if(f&&f.length===0){c()
}else{jQuery.when.apply(null,g).then(function(){c()
})
}});