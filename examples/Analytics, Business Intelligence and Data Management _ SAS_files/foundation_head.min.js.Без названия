function getUrlParams(){var c=[],d,b,a;
var e=window.location.href.slice(window.location.href.indexOf("?")+1).split("&");
a=e.length;
for(b=0;
b<a;
b+=1){d=e[b].split("=");
c.push(d[0]);
c[d[0]]=d[1]
}return c
}function getHostName(){var a=""+window.location;
var b=a.match(/^https?:\/\/[^\/]+/);
return b
}jQuery(document).ready(function(){jQuery("body").moveDescriptions()
