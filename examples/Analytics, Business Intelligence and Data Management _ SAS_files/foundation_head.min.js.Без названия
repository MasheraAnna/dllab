function getDropDownListToUse(){if(typeof(SAS.forms.dropdownListPath)==="undefined"||SAS.forms.dropdownListPath===""){SAS.forms.dropdownListPath="/etc/acs-commons/lists/statedropdowns"
}return SAS.forms.dropdownListPath
}function getStateFieldName(b){var a="";
if(b.search(/country/)!=-1){a=b.replace("country","state")
}else{if(b.search(/Country/)!=-1){a=b.replace("Country","State")
}else{if(b.search(/COUNTRY/)!=-1){a=b.replace("COUNTRY","STATE")
}}}return a
}function clearStateOptions(a){var b=jQuery("select[name="+a+"]");
if(b===undefined||b===null){return
}jQuery(b).find("option").remove();
return b
}function populateStates(d,m){var e,h;
if(d===undefined||d===null){return
}var k=clearStateOptions(m);
var c;
var f=document.getElementsByTagName("LABEL");
for(var e=0;
e<f.length;
e++){if(f[e].htmlFor.indexOf("_jcr_content_par_start_state")>-1){c=f[e]
}}if(c!=null){var l=JSON.stringify(d[0]);
if(l.indexOf('"value":"State"')>-1){var j=l.replace(/({\"text\":\")(.*)(\",\"value\":\")(.*)(\"})/g,"$2");
c.innerText=j
}}var a=null;
if(jQuery.browser.msie&&(jQuery.browser.version=="8.0"||jQuery.browser.version=="7.0"||jQuery.browser.version=="6.0")){for(e=1,h=d.length;
e<h;
e++){var g=JSON.stringify(d[e]);
var b=g.replace(/({\"text\":\")(.*)(\",\"value\":\")(.*)(\"})/g,"$4=$2");
a=b.split("=");
if(a.length<2){a.push(a[0])
}jQuery("<option/>",{value:a[0],text:a[1]}).appendTo("select[name="+m+"]")
}}else{for(e=1,h=d.length;
e<h;
e++){var g=JSON.stringify(d[e]);
var b=g.replace(/({\"text\":\")(.*)(\",\"value\":\")(.*)(\"})/g,"$4=$2");
a=b.split("=");
if(a.length<2){a.push(a[0])
}k.append(new Option(a[1],a[0]))
}}};