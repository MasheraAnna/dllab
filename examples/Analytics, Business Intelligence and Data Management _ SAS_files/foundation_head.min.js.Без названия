jQuery("div.form_leftcolmark:contains('*')").each(function(c){var b='<span class="required">*</span>';
jQuery(this).prev("div.form_leftcollabel").find("label").append(b);
a=jQuery(this).parent().parent().parent().find("div.form_error").text();
jQuery(this).hide();
var d=jQuery('<span class="form_error">'+a+"</span>");
if(a){jQuery(this).closest("div.form_row").find("input,select,textarea").each(function(e){jQuery(this).last().after(d);
jQuery(this).addClass("required")
})
}});
jQuery("div.form_error:parent").hide()
};
