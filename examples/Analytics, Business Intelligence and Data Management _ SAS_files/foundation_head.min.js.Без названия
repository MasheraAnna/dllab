function isOptedIn(b){if(!b){return false
}var a=false;
jQuery.ajax({url:"/services/optInVerification?email="+b,dataType:"json",async:false,success:function(d,c,e){if(d){a=d
}},error:function(e,d,c){}});
return a
}function isInputTypeClickable(a){return a&&(a==="radio"||a==="checkbox")
}function createEvent(c,b,d,a){var f=jQuery.Event(c,{});
var e=jQuery.Event(c,{newValue:d,oldValue:b,propertyName:"value",originalEvent:f});
return e
}jQuery(function(b){var a=function(){var e=b('input:hidden[name=":formstart"]');
