jQuery(function(a){a(document).ready(function(){a("select").filter(function(){return this.name.match(/country/)
}).change(function(){var e=a(this).attr("name");
var c=getStateFieldName(e);
var d=a(this).val().toLowerCase();
var b=getDropDownListToUse();
a.ajax({url:"/services/statelist.json",data:{countryVal:d,dropPath:b},dataType:"json",async:false,success:function(g,f,h){populateStates(g,c)
},error:function(h,g,f){clearStateOptions()
}})
});
