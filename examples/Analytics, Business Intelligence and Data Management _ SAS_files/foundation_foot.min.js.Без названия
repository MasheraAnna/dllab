if(b&&b.val()=="async"){jQuery.ajax({url:f.attr("action"),type:"POST",data:f.serialize(),success:function(h){f.replaceWith(c.html());
try{handlePostAsyncFormSubmit()
}catch(g){console.log("post async handler method does not exist for the current design")
}return false
},error:function(g){console.log(g.statusText+" "+g.responseText);
return false
}});
return false
}else{f.submit();
var d=ContextHub.getStore("sas_aem_forms_data");
if(d.putRegistration(f)){a.setcookiereg("sasevents",d.getEventID(),a.Utils.Base64.encode(d.getEmail()))
}}};
return a
}(SAS||{}));